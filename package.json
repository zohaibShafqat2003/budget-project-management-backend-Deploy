{
  "name": "budget-project-management-api",
  "version": "1.0.0",
  "description": "Backend API for Budget Project Management System",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "lint": "eslint .",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "custom-seed": "node seed.js",
    "create-migration": "npx sequelize-cli migration:generate --name",
    "create-seed": "npx sequelize-cli seed:generate --name",
    "db:reset": "cross-env FORCE_DB_SYNC=true ALLOW_DB_RESET=true node src/server.js",
    "init-db": "node scripts/force-sync-db.js",
    "setup-db": "node scripts/setup-database.js",
    "test-workflow": "node scripts/test-workflow.js",
    "test-transitions": "node scripts/test-workflow-transitions.js",
    "test-budget": "node scripts/test-budget-summary.js",
    "test-rbac": "node scripts/test-rbac.js",
    "test-all": "node scripts/run-all-tests.js"
  },
  "keywords": [
    "project-management",
    "budget",
    "api",
    "node",
    "express",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "@google/generative-ai": "^0.24.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.2.1",
    "file-saver": "^2.0.5",
    "file-type": "^16.5.4",
    "helmet": "^6.1.5",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pdfkit": "^0.17.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
